@{
}

@model WebUI.Models.StoreFrontVM

<h1 class="display-4">Create StoreFront</h1>



<form asp-action="Create">
    <div class="mb-3">
        <div asp-validation-summary=" ModelOnly"></div>
        <label  asp-for="Name"class="col-form-label">Name</label>
        <input asp-for="Name" type="text" class="form-control" />
        <span asp-validation-for="Name"></span>
    </div>

    <div class="mb-3">
        <label asp-for="Address" class="col-form-label">Address</label>
        <input asp-for="Address" type="text" class="form-control" />
        <span asp-validation-for="Address"></span>
    </div>

    <button type="submit" class="btn btn-primary" value="Create">Submit</button>
</form>

